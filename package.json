{
  "name": "tinybench",
  "version": "2.9.0",
  "type": "module",
  "packageManager": "pnpm@9.12.1",
  "volta": {
    "node": "20.18.0",
    "pnpm": "9.12.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepare": "git config core.hooksPath .hooks",
    "publish": "pnpm build && clean-publish",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src test examples tsup.config.ts",
    "lint:fix": "eslint --fix src test examples tsup.config.ts",
    "release": "bumpp package.json --commit --push --tag && pnpm publish",
    "test": "vitest --retry=5 --run"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "repository": "tinylibs/tinybench",
  "license": "MIT",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@size-limit/time": "^7.0.8",
    "@types/node": "^18.19.55",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "bumpp": "^9.7.1",
    "changelogithub": "^0.13.11",
    "clean-publish": "^5.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "nano-staged": "^0.8.0",
    "p-limit": "^6.1.0",
    "size-limit": "^7.0.8",
    "tsup": "^8.3.0",
    "typescript": "~5.5.4",
    "vitest": "^2.1.3"
  },
  "keywords": [
    "benchmark",
    "tinylibs",
    "tiny"
  ]
}
